<odoo>
  <template id="custom_snippet_group" inherit_id="website.snippets">
    <xpath expr="//snippets[@id='snippet_groups']" position="inside">
      <t snippet-group="custom_snippet" t-snippet="website.s_snippet_group" string="Custom snippet"
         t-thumbnail="/mi_modulo/static/src/img/snippets_thumbs/mi_grupo.svg"/>
    </xpath>
  </template>
</odoo>


        
        
        
        