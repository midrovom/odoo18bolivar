<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="s_cover_custom" inherit_id="theme_prime.snippets" name="Cover Custom - Registry">
        <xpath expr="//t[@t-snippet='theme_prime.s_banner_15']" position="after">
            <t t-snippet="theme_prime_image.s_banner_16" string="Banner Custom" group="theme_prime_banner">
                <keywords>covers, carousel</keywords>
            </t>
        </xpath>
    </template>

    <template id="s_banner_16" name="Banner - 16">
        <section class="s_banner_10 parallax s_parallax_is_fixed pt56 pb56 s_parallax_no_overflow_hidden" 
                data-scroll-background-ratio="0" data-snippet="s_banner_10" data-name="Banner - 10">
            <span class="s_parallax_bg oe_img_bg o_bg_img_center" 
                style="background-image: url('/web/image/theme_prime.s_banner_10_1'); background-position: 50% 0%;"/>
            <div class="container">
                <div class="row s_col_no_bgcolor s_nb_column_fixed">
                    <div class="col-12">
                    </div>
                </div>
            </div>
        </section>
    </template>






</odoo>
