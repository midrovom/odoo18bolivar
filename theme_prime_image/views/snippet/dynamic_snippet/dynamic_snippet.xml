<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="my_snippets" inherit_id="website.snippets">
    <xpath expr="//t[@id='installed_snippets_hook']" position="after">
        <t snippet-group="my_dynamic" t-snippet="website.s_snippet_group" string="My Dynamic Snippets" t-thumbnail="/my_module/static/src/img/snippets_thumbs/my_dynamic.svg"/>
    </xpath>

    <t t-snippet="website.s_banner" position="before">
        <t t-snippet="my_module.my_s_d_products_snippet" string="My Products" group="my_dynamic">
            <keywords>products, dynamic</keywords>
        </t>
    </t>
</template>
</odoo>

